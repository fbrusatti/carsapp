	</tr>
	<tr>
		<td style="color:#DF3A01">Marca</td>
		<td>{{mark}}</td>
	</tr>   
        <tr>
		<td style="color:#DF3A01">Kilometros</td>
		<td>{{km}}</td>
	</tr>
        <tr>
		<td style="color:#DF3A01"> Color</td>
		<td>{{color}}</td>
	</tr>

	</table>

	<a href="http://localhost:4567/ListUser" style="color:#DF3A01"><h3>Volver </h3> </a> 
